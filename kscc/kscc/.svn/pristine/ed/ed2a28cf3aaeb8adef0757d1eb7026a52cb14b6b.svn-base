            </form>
         </div>
         <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
            <button type="button" class="btn btn-primary saveAdd">确定</button>
         </div>
      </div><!-- /.modal-content -->
   </div><!-- /.modal -->
</div>

