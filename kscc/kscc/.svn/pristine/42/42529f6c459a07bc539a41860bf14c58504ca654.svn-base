	var addUserList = function(data){
		if(!data){return;}
		var userList = $("#liveParticipant");
		var uid = data.data;
		var uv = data.value;
		if(userList.find("li[uid='"+uid+"']").length>0){
			alert(errorStr);
			return;
		}
		var li = document.createElement("li");
		$(li).attr("uid",uid);
		$(li).attr("uv",uv);
		$(li).html("<i class='fa fa-user-circle'></i>"+uv+"<i class='fa fa-remove' style='float:right;margin-top:5px;cursor:pointer;'></i>");
		$(userList).append(li);
		$(li).find("i:eq(1)").on("click",function(event){
			event.stopPropagation();
			delUser(uid);
		});
		$(li).find("i:eq(1)").unbind("mousedown").on("mousedown",function(event){
			return false;
		});
		$(li).unbind("mousedown").on("mousedown",function(event){
			event.stopPropagation();
			initDrag(this);
			setDrag(this,event);
			$(this).css("cursor","move");
			$("body").css("cursor","move");
		});
	};

	var delUser = function(uid){
		$("#liveParticipant li[uid='"+uid+"']").remove();
	};
	
